{
  "schema_version": "1.0.0",
  "plugins": {
    "kubecost": {
      "name": "kubecost",
      "description": "Kubernetes cost analysis via Kubecost API",
      "repository": "rshade/finfocus-plugin-kubecost",
      "author": "FinFocus Team",
      "license": "Apache-2.0",
      "homepage": "https://github.com/rshade/finfocus-plugin-kubecost",
      "supported_providers": ["kubernetes"],
      "capabilities": ["cost_retrieval", "cost_projection"],
      "security_level": "official",
      "min_spec_version": "0.1.0"
    },
    "aws-public": {
      "name": "aws-public",
      "description": "AWS public pricing data",
      "repository": "rshade/finfocus-plugin-aws-public",
      "author": "FinFocus Team",
      "license": "Apache-2.0",
      "homepage": "https://github.com/rshade/finfocus-plugin-aws-public",
      "supported_providers": ["aws"],
      "capabilities": ["cost_projection", "pricing_specs", "recommendations"],
      "security_level": "official",
      "min_spec_version": "0.1.0",
      "asset_hints": {
        "asset_prefix": "finfocus-plugin-aws-public",
        "default_region": "us-east-1",
        "version_prefix": false
      }
    }
  }
}
